{#
/**
 * @file
 * Template for a dart core 1 column layout.
 */
#}
<div class="blog-masthead">
  <div class="container">
    {{ content.header }}
  </div>
</div>

<div class="container">

  <div class="blog-header">
    {{ content.featured }}
  </div>

  <div class="row">
    <div class="col-sm-12 blog-main">
      {{ content.content }}
    </div>
  </div>

</div>

<footer class="blog-footer">
  {{ content.footer }}
</footer>
